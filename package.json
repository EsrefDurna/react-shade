{
  "babel": {
    "env": {
      "es": {
        "ignore": [
          "__tests__",
          "node_modules"
        ],
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ],
          "flow",
          "react",
          "stage-0"
        ]
      },
      "esnext": {
        "ignore": [
          "__tests__",
          "node_modules"
        ],
        "presets": [
          "es2016",
          "es2017",
          "flow",
          "react",
          "stage-0"
        ]
      },
      "node": {
        "ignore": [
          "__tests__",
          "node_modules"
        ],
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "6.0.0"
              }
            }
          ],
          "flow",
          "react",
          "stage-0"
        ]
      },
      "test": {
        "presets": [
          "env",
          "flow",
          "react",
          "stage-0"
        ]
      }
    }
  },
  "devDependencies": {
    "@skatejs/ssr": "^0.17.2",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "conartist": "^0.13.2",
    "jest": "^20.0.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "files": [
    "es/",
    "esnext/",
    "node/"
  ],
  "main": "index.js",
  "module": "es/index.js",
  "esnext": "esnext/index.js",
  "scripts": {
    "build": "npm run build:es && npm run build:esnext && npm run build:node",
    "build:es": "BABEL_ENV=es babel src --out-dir es",
    "build:esnext": "BABEL_ENV=esnext babel src --out-dir esnext",
    "build:node": "BABEL_ENV=node babel src --out-dir node",
    "prepublish": "npm run build",
    "test": "jest"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "./node_modules"
    ],
    "setupFiles": [
      "./jest"
    ],
    "testEnvironment": "@skatejs/ssr/jest"
  },
  "name": "react-shade",
  "version": "0.0.1",
  "description": "",
  "author": "Trey Shugart <treshugart@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.6.0"
  }
}
